  "version": "1.0.0",
  "engines": {
    "node": "10.2.1",
    "yarn": "1.9.4"
  },
  "description": "Battleship api.",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "build": "ts-node build.ts && tsc",
    "dist": "NODE_ENV=production webpack --config webpack.config.js",
    "start-dev": "NODE_END=development nodemon server/index.ts | pino-pretty",
    "dev:debug": "nodemon --exec 'node -r ts-node/register --inspect-brk' server/index.ts | pino-pretty",
    "test": "mocha test/**/*.ts --exit",
    "test:coverage": "mocha test/**/*.ts --exit",
    "test:watch": "mocha --inspect-brk test/**/*.ts --exit"
  },
  "dependencies": {
    "bcrypt": "^3.0.5",
    "bluebird": "^3.5.3",
    "body-parser": "^1.18.3",
    "config": "^3.0.1",
    "cookie-parser": "^1.4.4",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "http-status-codes": "^1.3.2",
    "jsonwebtoken": "^8.5.1",
    "lodash.pick": "^4.4.0",
    "mongoose": "^5.4.19",
    "mysql": "^2.16.0",
    "pino": "^5.11.1",
    "swagger-express-middleware": "^2.0.1"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.26",
    "@types/chai": "^4.1.7",
    "@types/express": "^4.16.1",
    "@types/mocha": "^5.2.6",
    "@types/mongoose": "^5.3.22",
    "@types/mysql": "^2.15.5",
    "@types/pino": "^5.8.5",
    "chai": "^4.2.0",
    "mocha": "^6.0.2",
    "nodemon": "^1.18.10",
    "pino-pretty": "^2.5.0",
    "shelljs": "^0.8.3",
    "supertest": "^3.4.2",
    "ts-lint": "^4.5.1",
    "ts-node": "^8.0.2",
    "tslint": "^5.13.1",
    "typescript": "^3.3.3333"
  }
}
